<%= form_with model: @instructor do |f|%>
  <%= render "shared/error_messages", resource: @instructor %>

  <%= f.text_field :name, placeholder: 'Nome'%>
  <%= f.text_field :email, placeholder: 'Email'%>  
  <%= f.text_area :bio, placeholder: 'Descrição'%>
  <%= f.label :profile_picture, 'Foto de perfil' %>
  <%= f.file_field :profile_picture %>
  <%= f.submit 'Salvar' %>
  <%= link_to "Excluir", instructors_path %>
<% end %>