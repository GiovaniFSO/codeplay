<%= form_with model: @instructor do |f|%>
  <%= render "shared/error_messages", resource: @instructor %>
  <%= f.label :name%>
  <%= f.text_field :name%>
  <%= f.label :email%>
  <%= f.text_field :email%>  
  <%= f.label :bio%>
  <%= f.text_area :bio%>
  <%= f.label :profile_picture %>
  <%= f.file_field :profile_picture %>
  <%= f.submit 'Salvar' %>
  <%= link_to "Excluir", instructors_path %>
<% end %>